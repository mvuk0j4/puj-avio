<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.puj.controller.MenuController">
   <children>
      <ButtonBar layoutX="14.0" layoutY="4.0" prefHeight="50.0" prefWidth="356.0">
        <buttons>
          <Button fx:id="reservationBtn" mnemonicParsing="false" onAction="#openReservation" prefHeight="24.0" prefWidth="101.0" text="Rezervacije" />
            <Button fx:id="passangerBtn" layoutX="229.0" layoutY="23.0" mnemonicParsing="false" onAction="#openPassanger" text="Putnici" />
            <Button fx:id="planeBtn" layoutX="229.0" layoutY="23.0" mnemonicParsing="false" onAction="#openPlane" prefHeight="24.0" prefWidth="146.0" text="Avioni" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
